{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2022",
    "typeRoots": [
      "../../examples/vue/node_modules/@types",
      "../../examples/vue/node_modules",
      "../../npm-packages"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "preserveSymlinks": true,
    "strict": true,
    "baseUrl": "../../examples/vue/",
    "paths": {
      "meteor/*": [
        "../../examples/vue/node_modules/@types/meteor/*",
        "../../examples/vue/.meteor/local/types/packages.d.ts",
        ".npm/plugin/vite/node_modules/@types"
      ]
    },
    "skipLibCheck": true
  }
}
